{
  "name": "xander-global-scholars",
  "version": "1.0.0",
  "scripts": {
    "predev": "export NODE_OPTIONS=--openssl-legacy-provider || set NODE_OPTIONS=--openssl-legacy-provider",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "start": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm install --production"
  },
  "devDependencies": {
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "concurrently": "^8.0.1",
    "nodemon": "^3.1.7"
  },
  "dependencies": {
    "16": "^0.0.2",
    "18": "^0.0.0",
    "python": "^0.0.4",
    "sass": "^1.82.0"
  }
}
